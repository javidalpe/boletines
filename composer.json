{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "algolia/algoliasearch-client-php": "^2.5.0",
        "doctrine/dbal": "^2.10.0",
        "guzzlehttp/guzzle": "^6.4.1",
        "html2text/html2text": "^4.2.1",
        "htmlmin/htmlmin": "^6.0.1",
        "laracasts/flash": "^3.0.3",
        "laravel/cashier": "^10.5.1",
        "laravel/framework": "6.6.*",
        "laravel/scout": "^7.2.1",
        "laravel/socialite": "^4.3.1",
        "laravel/tinker": "2.0.*",
        "laravelcollective/html": "^6.0.3",
        "league/flysysteTrustProxiesm-aws-s3-v3": "^1.0.23",
        "prettus/l5-repository": "2.6.*",
        "sentry/sentry-laravel": "1.5.*",
        "smalot/pdfparser": "0.14.*",
        "socialiteproviders/google": "3.0.*",
        "spatie/laravel-cookie-consent": "2.10.*",
        "spatie/laravel-sitemap": "5.5.*",
        "stripe/stripe-php": "7.14.*"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.6.*",
        "filp/whoops": "2.5.*",
        "fzaninotto/faker": "1.9.*",
        "laravel/telescope": "2.0.*",
        "mockery/mockery": "1.3.*",
        "phpunit/phpunit": "8.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
